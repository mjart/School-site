        <div id="extern"><script type="text/javascript">
$(document).ready(function(){
	$('input[name=zoeken]').focus(function() {
		$this = $(this);
		if ($this.val() == "Zoeken") {
			$this.val('');
		}
	});
	$('input[name=zoeken]').focusout(function() {
		$this = $(this);
		if ($this.val() == "") {
			$this.val("Zoeken");
		}
	}); 
	// Minimaal aantal tekens voor het zoeken
	var minimum_chars = 2;
	$('.zoek_form').submit(function(event) {			
		if($("input[name=zoeken]").val().length >= minimum_chars) {			
			
		} else {
			event.preventDefault();
			alert('Voor s.v.p. minimaal '+minimum_chars+' karakters in.')			
		}
	});				
});
</script>
