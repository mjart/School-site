				<script>
    $(document).ready(function () {
        $('img.des').unbind('mouseout');
        $('img.des').unbind('mouseover');
        $('canvas.des').unbind('mouseout');
        $('canvas.des').unbind('mouseover');
        $(".bloktekstopmaak").mouseenter(function (e) {
            $('canvas.des').unbind('mouseout');
            $('canvas.des').unbind('mouseover');
            $(this).find('.overlay').removeClass("trans");
            $(this).find('.bloktekst').addClass("bloktekstopmaakHover");
            $(this).find('canvas.des').hide();
            $(this).find('img.des').show();
        });
        $(".bloktekstopmaak").mouseleave(function (e) {
            $('canvas.des').unbind('mouseout');
            $('canvas.des').unbind('mouseover');
            $(this).find('.overlay').addClass("trans");
            $(this).find('.bloktekst').removeClass("bloktekstopmaakHover");
            $(this).find('canvas.des').show();
            $(this).find('img.des').hide();
        });
    });
</script>
